{
  "Description": "[Contract Test] create asymmetric",
  "KeyPolicy": {
    "Version": "2012-10-17",
    "Id": "key-default-1",
    "Statement": [
      {
        "Sid": "Enable IAM User Permissions",
        "Effect": "Allow",
        "Principal": {
          "AWS": "arn:aws:iam::${userAccount}:root"
        },
        "Action": "kms:*",
        "Resource": "*"
      }
    ]
  },
  "KeySpec": "ECC_NIST_P256",
  "KeyUsage": "SIGN_VERIFY",
  "PendingWindowInDays": 7,
  "Tags": [
    {
      "Key": "Key",
      "Value": "Value"
    }
  ]
}
